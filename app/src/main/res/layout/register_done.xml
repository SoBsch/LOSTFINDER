<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/register_width"
    android:layout_height="wrap_content"
    android:background="@null">


    <TextView
        android:id="@+id/register_main_message1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:letterSpacing="0.3"
        android:lineSpacingExtra="-15.5sp"
        android:text="COMPLETE"
        android:textColor="#3f9df4"
        android:textSize="19.3dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/register_main_message2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:letterSpacing="-0.03"
        android:lineSpacingExtra="5sp"
        android:text="@string/register_main_tv2"
        android:textColor="#cc000000"
        android:textSize="13.5sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/register_main_message1" />

    <!--사람이미지-->
    <ImageView
        android:id="@+id/register_main_person"
        android:layout_width="74.7dp"
        android:layout_height="74.7dp"
        android:layout_marginTop="29dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/register_main_message2" />

    <TextView
        android:id="@+id/register_main_submit"
        android:layout_width="0dp"
        android:layout_height="34.7dp"
        android:background="@drawable/login_round"
        android:gravity="center"
        android:text="로그인하기"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/register_main_person" />


</android.support.constraint.ConstraintLayout>