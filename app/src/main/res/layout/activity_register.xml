<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/login_background"
    tools:context=".ui.register.Register">


    <ImageView
        android:id="@+id/register_iv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24.3dp"
        android:layout_marginTop="30.7dp"
        android:src="@drawable/ic_menu_black_24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="회원가입"
        android:textColor="@color/black"
        android:textSize="19.3dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/register_iv1"
        app:layout_constraintStart_toEndOf="@id/register_iv1"
        app:layout_constraintTop_toTopOf="@id/register_iv1" />

    <FrameLayout
        android:id="@+id/register_container"
        android:layout_width="210.3dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="216.7dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0" />

    <ImageButton
        android:id="@+id/register_back"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_marginBottom="46.3dp"
        android:layout_marginStart="24.3dp"
        android:background="@drawable/register_back"
        android:src="@drawable/ic_keyboard_backspace_black_24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</android.support.constraint.ConstraintLayout>