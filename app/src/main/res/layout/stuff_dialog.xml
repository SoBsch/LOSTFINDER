<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="254dp"
    android:layout_height="133dp"
    android:layout_gravity="center"
    android:background="@color/login_tint">

    <TextView
        android:id="@+id/stuff_dialog_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="37.3dp"
        android:fontFamily="@font/basic"
        android:letterSpacing="0.1"
        android:text="등록 완료!"
        android:textColor="@color/white"
        android:textSize="14.8sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/stuff_dialog_submit"
        android:layout_width="134dp"
        android:layout_height="35dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/white_round"
        android:fontFamily="@font/basic"
        android:gravity="center"
        android:text="확인"
        android:textColor="@color/login_tint"
        android:textSize="14.8sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@id/stuff_dialog_title"
        app:layout_constraintStart_toStartOf="@id/stuff_dialog_title"
        app:layout_constraintTop_toBottomOf="@id/stuff_dialog_title" />

</android.support.constraint.ConstraintLayout>